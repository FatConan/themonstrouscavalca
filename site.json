{   
    "sites": [
    {
        "root": "themonstrouscavalca",
        "pages": [
            {
                "source": "pages/index.md",
                "target": "output/index.html"
            },
            {
                "source": "pages/entries/*.md",
                "target": "output/terrible-creations/*.html"
            },
            {
                "source": "pages/blog/*.md",
                "target": "output/ill-conceptions/*.html"
            },
            {
                "source": "pages/blog/the-garderobe/*.md",
                "target": "output/ill-conceptions/the-garderobe/*.html"
            },
            {
                "source": "pages/about/index.md",
                "target": "output/the-curator/index.html"
            },
            {
                "source": "pages/about/cv.md",
                "target": "output/the-curator/cv.html"
            },
            {
                "source": "pages/errors/404.md",
                "target": "output/404.shtml"
            }
        ],
        "templates": [
            "templates"
        ],
        "resources": [
            {
                "source": "resources/css/reset.css",
                "target": "output/resources/css/reset.css"
            },
            {
                "source": "resources/css/styles.css",
                "target": "output/resources/css/styles.css"
            },
            {
                "source": "resources/css/debug.css",
                "target": "output/resources/css/debug.css"
            },
            {
                "source": "resources/img",
                "target": "output/resources/img",
                "resource_type": "relocate"
            },
            {
                "source": "resources/scripts",
                "target": "output/resources/scripts",
                "resource_type": "relocate"
            }
        ]
    }
]}
